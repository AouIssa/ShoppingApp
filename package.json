{
  "name": "shoppingapp",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "e2e": "detox test -c ios.sim.release",
    "e2e:build": "expo build -t simulator --config app.json --non-interactive --no-publish --release-channel e2e",
    "e2e:install": "expo install:ios.simulator --archive ./path/to/your-app.tar.gz"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "detox": "^20.6.0",
    "expo": "~48.0.9",
    "expo-detox-hook": "^1.0.10",
    "expo-print": "~12.2.1",
    "expo-sharing": "~11.2.2",
    "expo-status-bar": "~1.4.4",
    "jest": "^29.5.0",
    "react": "18.2.0",
    "react-native": "0.71.4",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
